<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.org.mvp_mvvmpatterndemo.views.activities.MainActivity">

    <TextView
        android:id="@+id/textViewName"
        android:layout_marginTop="@dimen/dimen_margin_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:layout_marginBottom="@dimen/dimen_margin_bottom"
        android:textSize="@dimen/dimen_text_name"/>

    <TextView
        android:id="@+id/textViewEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewName"
        android:textColor="@color/black"
        android:layout_marginBottom="@dimen/dimen_margin_bottom"
        android:textSize="@dimen/dimen_text_name"/>

    <TextView
        android:id="@+id/textViewAddress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewEmail"
        android:textColor="@color/black"
        android:layout_marginBottom="@dimen/dimen_margin_bottom"
        android:textSize="@dimen/dimen_text_name"/>
</RelativeLayout>
